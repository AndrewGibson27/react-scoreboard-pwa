{
  "name": "react-scoreboard-ssr",
  "version": "0.1.0",
  "description": "Hand-spun React SSR with code splitting, Redux, GraphQL and JWT authentication",
  "repository": "https://github.com/AndrewGibson27/react-scoreboard-ssr",
  "author": "Andrew Gibson <atgibson27@gmail.com>",
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/AndrewGibson27/react-scoreboard-ssr/issues"
  },
  "scripts": {
    "start": "NODE_ENV=development BABEL_ENV=server babel-node ./src/entry/server.jsx",
    "start:prod": "NODE_ENV=production node ./build/server.js",
    "client:prod": "NODE_ENV=production BABEL_ENV=client webpack --config ./webpack/client.prod.js",
    "server:prod": "NODE_ENV=production BABEL_ENV=server webpack --config ./webpack/server.prod.js",
    "prod": "yarn run clean && yarn run client:prod && yarn run server:prod && yarn start:prod",
    "clean": "rm -rf build && rm -rf public/assets/"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-session": "^1.15.6",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.2",
    "history": "^4.7.2",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.0.13",
    "morgan": "^1.9.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "rimraf": "^2.6.2",
    "webpack": "^4.20.2",
    "webpack-assets-manifest": "^3.0.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-hot-middleware": "^2.22.0",
    "webpack-sources": "^1.0"
  }
}
